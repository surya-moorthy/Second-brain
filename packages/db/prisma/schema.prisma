
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String    @id @default(uuid())
  username String
  password String
  content  Content[]
}

model Content {
  id        String    @id @default(uuid())
  type      String    @default("")
  title     String
  link      String
  userId    String
  createdAt DateTime? @default(now())
  updatedAt DateTime? @default(now())
  user      User      @relation(fields: [userId], references: [id])
}
